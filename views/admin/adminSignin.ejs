  <script>
    const adminsigninForm = document.getElementById('adminsigninForm');
  
  // Attach an event listener to the form's submit event
  adminsigninForm.addEventListener('submit', function (event) {
    // Prevent the default form submission
    event.preventDefault();
  
    // Call the signupValidate function
    const isValid = adminloginValidate();
  
    // If validation fails, do not submit the form
    if (!isValid) {
      return false;
    } else {
      adminsigninForm.submit();
    }
    // If validation passes, you can manually submit the form
    // This will trigger the actual form submission to /register
  });
  </script>

